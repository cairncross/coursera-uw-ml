   "source": [
    "def contains_perfect(perfect_count):\n",
    "    if perfect_count >= 1:\n",
    "        return 1\n",
    "    else:\n",
    "        return 0\n",
    "    \n",
    "products['contains_perfect'] = products['perfect'].apply(lambda perfect_count : contains_perfect(perfect_count))\n",
    "\n",
    "print '# of rows containing perfect =', len(products[products['contains_perfect']==1])"
   ]
  },
